{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [ "airbnb", "prettier"],
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "import/prefer-default-export" : ["off"],
      "no-debugger" : [1]
    }
}
