{
    "extends": ["react-app", "prettier", "prettier/react", "airbnb"],
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
          "error",
          {
            "singleQuote": true
          }
        ],
        "react/jsx-filename-extension": [
          "warn",
          {
            "extensions": [".js", ".jsx"]
          }
        ]
      }
}
